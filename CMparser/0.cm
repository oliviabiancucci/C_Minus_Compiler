int add(int a);
int sub2(int a);

int add(int a){
  int z;

  if(a > 5)
  {
    z = sub2(a + 1);
    return z;
  }
  else{
    return z;
  }
}

int sub2(int a){
  int z;

  if(a > 5)
  {
    z = add(a - 2);
    return z;
  }
  else{
    return z;
  }
}

void main(void) {
  int x; 
  int z;

  x = 8;
  z = add(x);
}
